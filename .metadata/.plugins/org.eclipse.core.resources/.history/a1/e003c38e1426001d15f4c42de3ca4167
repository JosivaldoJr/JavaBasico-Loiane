import java.util.*;

public class Q04 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		double popA = 80000, taxA = 0.03;
		double popB = 200000, taxB = 0.015;
		int anos = 0;
		System.out.println();
		while(popA < popB) {
			popA = (popA*taxA)+popA;
			System.out.println("População A: "+popA);
			popB = (popB*taxB)+popB;
			System.out.println("População B: "+popB);
			
			anos++;
		}
		
		System.out.println("Quantidade de anos: "+anos);
		double a=15, b=100;
		double conta = (a/b);
		System.out.println(conta);
		
		
	}

}
