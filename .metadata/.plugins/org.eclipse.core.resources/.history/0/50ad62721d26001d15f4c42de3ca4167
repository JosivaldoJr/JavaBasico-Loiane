import java.util.*;

public class Q07 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num;
		int maior = Integer.MIN_VALUE;
		
		for(int i=0; i<5; i++) {
			System.out.println("Digite: ");
			num = sc.nextInt();
			
			if(num > maior) {
				num = maior;
				
			}
		}
		
		System.out.println("Maior número: "+maior);

	}

}
